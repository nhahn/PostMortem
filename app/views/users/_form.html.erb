<%= simple_form_for(@user) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :email %>
    <%= f.input :dob %>
    <%= f.input :street %>
    <%= f.input :zip %>
    <%= f.input :state %>
    <%= f.input :phone %>
    <%= f.input :password_digest %>
    <%= f.input :last_name %>
    <%= f.input :first_name %>
    <%= f.input :deceased %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
